{
  "name": "velocity303-netbt",
  "version": "0.0.1",
  "author": "James Jones",
  "summary": "This module modifies the netbiosoptions registry setting for all interfaces on a Windows system.",
  "license": "Apache-2.0",
  "source": "https://github.com/velocity303/puppet_netbt.git",
  "project_page": "https://github.com/velocity303/puppet_netbt",
  "issues_url": "https://github.com/velocity303/puppet_netbt/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Windows",
      "operatingsystemrelease": [
        "Server 2003",
        "Server 2003 R2",
        "Server 2008",
        "Server 2008 R2",
        "Server 2012",
        "Server 2012 R2",
        "7",
        "8"
        "10"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 2015.1.0 < 2015.4.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    }
  ],
  "description": "This module modifies the netbiosoptions registry setting for all interfaces on a Windows system.",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 2.3.0"
    },
    {
      "name": "puppetlabs/registry",
      "version_requirement": ">= 1.1.3"
    }
  ]
}
